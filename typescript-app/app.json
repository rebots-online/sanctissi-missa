{
  "expo": {
    "name": "Sanctissi-Missa",
    "slug": "sanctissi-missa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/your-project-id"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "world.robinsai.sanctissimissa.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to save generated PDFs to your device.",
        "NSPhotoLibraryAddUsageDescription": "We need access to save generated PDFs to your device.",
        "UIBackgroundModes": ["fetch"]
      }
    },
    "android": {
      "package": "world.robinsai.sanctissi-missa.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "sanctissi-missa"
      }
    },
    "owner": "robinsai.world",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
